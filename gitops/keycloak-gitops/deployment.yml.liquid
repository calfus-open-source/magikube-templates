---
apiVersion: apps/v1
kind: Deployment
metadata:
  name: keycloak-service-deployment
  namespace: 
  labels:
    app: keycloak-service
    version: 6716dfb613d10c287062dcd5519758d113aa933e
spec:
  replicas: 1
  selector:
    matchLabels:
      app: keycloak-service
  progressDeadlineSeconds: 150
  template:
    metadata:
      annotations:
        cluster-autoscaler.kubernetes.io/safe-to-evict: "true"
        prometheus.io/scrape: "true"
        prometheus.io/custom: "true"
      labels:
        app: keycloak-service
        version: 6716dfb613d10c287062dcd5519758d113aa933e
    spec:
      containers:
        - name: keycloak-service
          env:
            - name: KC_HEALTH_ENABLED
              value: "true"
            - name: KC_METRICS_ENABLED
              value: "true"
            - name: KC_DB
              value: "postgres"
            - name: KC_DB_URL_DATABASE
              value: "keycloak"
            - name: KC_DB_URL
              value: ""
            - name: KC_HOSTNAME_ADMIN_URL
              value: ""
            - name: KC_HOSTNAME_URL
              value: ""         
            - name: KEYCLOAK_ADMIN
              value: ""
            - name: KEYCLOAK_ADMIN_PASSWORD
              value: ""
            - name: KC_DB_URL_HOST
              value: ""
            - name: KC_DB_URL_PORT
              value: "5432"
            - name: KC_DB_USERNAME
              value: ""
            - name: KC_DB_PASSWORD
              value: ""
            - name: PROXY_ADDRESS_FORWARDING
              value: "true"
            - name: KC_HTTP_ENABLED
              value: "true"
            - name: KC_PROXY
              value: "edge"
            - name: QUARKUS_TRANSACTION_MANAGER_ENABLE_RECOVERY
              value: "true"
            - name: KC_PROXY_HEADERS
              value: "xforwarded"
            - name: KC_TRANSACTION_XA_ENABLED
              value: "true"
          image: 905418022678.dkr.ecr.us-east-1.amazonaws.com/keycloak-service:6716dfb613d10c287062dcd5519758d113aa933e
          ports:
            - containerPort: 8080
          resources:
            requests:
              cpu: "500m"
              memory: "512Mi"

