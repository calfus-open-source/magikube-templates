apiVersion: apps/v1
kind: Deployment
metadata:
  name: react-service
  labels:
    app: react-service
spec:
  replicas: 2
  selector:
    matchLabels:
      app: react-service
  template:
    metadata:
      labels:
        app: react-service
    spec:
      containers:
      - name: react-service
        image: {{ .Values.accountId }}.dkr.ecr.{{ .Values.region }}.amazonaws.com/react-service:{{ .Values.imageHash }}
        ports:
        - containerPort: 80