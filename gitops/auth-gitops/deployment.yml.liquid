apiVersion: apps/v1
kind: Deployment
metadata:
  name: auth-service
  labels:
    app: auth-service
spec:
  replicas: 2
  selector:
    matchLabels:
      app: auth-service
  template:
    metadata:
      labels:
        app: auth-service
    spec:
      containers:
      - name: auth-service
        image: {{ .Values.accountId }}.dkr.ecr.{{ .Values.region }}.amazonaws.com/auth-service:{{ .Values.imageHash }}
        ports:
        - containerPort: 80