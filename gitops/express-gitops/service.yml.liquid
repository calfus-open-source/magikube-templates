---
kind: Service
apiVersion: v1
metadata:
  namespace: {{ project_name }}-{{ environment }}-app
  name: node-service-svc
  labels:
    app: node-service
    service: node-service-svc
spec:
  selector:
    app: node-service
  type: ClusterIP
  ports:
  - name: http-app
    protocol: TCP
    port: 3001
    targetPort: 3001