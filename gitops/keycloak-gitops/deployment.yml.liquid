apiVersion: apps/v1
kind: Deployment
metadata:
  name: keycloak-service
  labels:
    app: keycloak-service
spec:
  replicas: 1
  selector:
    matchLabels:
      app: keycloak-service
  template:
    metadata:
      labels:
        app: keycloak-service
    spec:
      containers:
      - name: keycloak-service
        image: {{ .Values.accountId }}.dkr.ecr.{{ aws_region}}.amazonaws.com/{{ project_name }}-{{ keycloak-service }}-app:{{ latest }}
        ports:
        - containerPort: 80